cd

find -perm -002 -ls

find /usr/share/doc -mtime +300

mkdir /tmp/files
find /usr/share -size +5M -size -10M
find /usr/share -size +5M -size -10M -exec cp {} /tmp/files

find /tmp/files -exec cp {} {}.mybackup

cd /usr/share/doc/kernel.doc
